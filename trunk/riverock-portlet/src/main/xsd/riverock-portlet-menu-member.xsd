<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Copyright 2002-2003, jSmithy.com, http://www.jSmithy.com</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>WebMill XML/XSLT web portlet engine</xs:documentation>
	</xs:annotation>
	<xs:complexType name="MenuMemberApplicationType">
		<xs:sequence>
			<xs:element name="ApplicationName" type="xs:string"/>
			<xs:element name="ApplicationRecordNumber" type="xs:int"/>
			<xs:element name="MenuMemberModule" type="MenuMemberModuleType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="orderField" type="xs:int"/>
		<xs:attribute name="applicationId" type="xs:long"/>
	</xs:complexType>
	<xs:complexType name="MenuMemberModuleType">
		<xs:sequence>
			<xs:element name="ApplRecordNumber" type="xs:int"/>
			<xs:element name="ModuleRecordNumber" type="xs:int"/>
			<xs:element name="ModuleName" type="xs:string"/>
			<xs:element name="ModuleUrl" type="xs:string"/>
		</xs:sequence>
		<xs:attribute name="orderField" type="xs:int"/>
	</xs:complexType>
	<xs:complexType name="MenuMemberType">
		<xs:sequence>
			<xs:element name="MenuMemberApplication" type="MenuMemberApplicationType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
