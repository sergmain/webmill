<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
    "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>

    <!-- User navigation rules -->
    <navigation-rule>
        <from-view-id>/riverock/manager/index.jsp</from-view-id>

        <navigation-case>
            <from-outcome>auth</from-outcome>
            <to-view-id>/riverock/manager/auth/auth.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>role</from-outcome>
            <to-view-id>/riverock/manager/role/role.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>company</from-outcome>
            <to-view-id>/riverock/manager/company/company.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>holding</from-outcome>
            <to-view-id>/riverock/manager/holding/holding.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>portlet-name</from-outcome>
            <to-view-id>/riverock/manager/portlet-name/portlet-name.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>portal-user</from-outcome>
            <to-view-id>/riverock/manager/users/portal-user.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>site-list</from-outcome>
            <to-view-id>/riverock/manager/site/site.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
