<xs:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Copyright 2002-2003, jSmithy.com, http://www.jSmithy.com</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation>WebMill XML/XSLT web portlet engine</xs:documentation>
	</xs:annotation>
	<xs:complexType name="TestSchema">
		<xs:sequence>
			<xs:element name="Name" type="xs:string"/>
			<xs:element name="bytes" type="xs:hexBinary" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TestLanguage" type="LanguageListType"/>
	<xs:complexType name="LanguageListType">
		<xs:sequence>
			<xs:element name="data" type="LanguageItemType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LanguageItemType">
		<xs:sequence>
			<xs:element name="NameLanguage" type="xs:string"/>
			<xs:element name="String" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="resource" type="PairListType"/>
	<xs:complexType name="PairListType">
		<xs:sequence>
			<xs:element name="pair" type="PairType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PairType">
		<xs:attribute name="key" type="xs:string"/>
		<xs:attribute name="value" type="xs:string"/>
	</xs:complexType>
</xs:schema>
