    ID_SERVICE
 FROM AUTH_USER
</Text>
			</View>
			<View schema="MILLENNIUM" name="F_D_V_B2C_ITEMS_PRICE_CURRENCY" type="VIEW">
				<Text xmlns="">select
    a.id_item, a.is_group, a.id, a.id_main,
    a.absolete,
    a.item,
    a.id_shop,
    e.currency,
    e.name_currency,
    decode
    (
       ids1, id_std_currency, a.price,
       vc1/vc2*a.price
    ) price,
    a.is_manual, a.is_special,
    c.id_std_currency,
    ids1, a.price price_orig, vc1, vc2,
    e.id_currency,
    a.quantity,
    e.id_site
from   price_list a, price_shop_table a1, cash_currency b,
       v_cash_client_cross_curs c,
       site_list_site d, cash_currency e
where
       a.absolete = 0 and
       a.is_group = 0 and
       a.currency = b.currency and  -- получаем ID валюты вместо кода
       c.id_std_currency = e.id_currency and
       a.id_shop = a1.id_shop and a1.id_site = d.id_site and
       d.id_site = b.id_site and
       b.id_currency = c.ids1
</Text>
			</View>
			<View schema="MILLENNIUM" name="F_D_V_CASH_CLIENT_CROSS_CURS" type="VIEW">
				<Text xmlns="">select
    a.id_currency ids1,
    b.id_currency id_std_currency,
    a.real_curs_percent vc1,
    b.real_curs_percent vc2
from
(
SELECT a1.*
FROM v_cash_currency_data a1 
) a,
(
SELECT a2.*
FROM v_cash_currency_data a2
) b
where a.id_site = b.id_site
</Text>
			</View>
			<View schema="MILLENNIUM" name="F_D_V_CASH_CLIENT_CURR_NULL" type="VIEW">
				<Text xmlns="">select  a.id_site, a.id_currency,
        a.name_currency||', курс 1:'||to_char(a.real_curs_percent) name_currency,
        a.is_used,
        a.name_currency currency_name,
        a.real_curs_percent currency_curs
from    v_cash_currency_data a
/*
union
select  distinct b.id_site, 0, 'без пересчета', 1, '', 0
from    site_list_site b
*/
</Text>
			</View>
			<View schema="MILLENNIUM" name="F_D_V_CASH_CURRENCY_DATA" type="VIEW">
				<Text xmlns="">select
    b.id_currency,
    b.name_currency,
    b.currency,
    a.date_change,
    a.curs current_curs,
    c.value_curs std_curs,
    c.date_change std_date_change,
    decode(b.is_use_standart, 1, c.value_curs, a.curs) real_curs,
    decode(b.is_use_standart, 1, c.value_curs, a.curs)*
        ((100+b.percent)/100) real_curs_percent,
    b.is_used,
    b.is_use_standart,
    c.name_std_curr,
    b.percent,
    b.id_site
    
FROM    cash_currency b, v_cash_select_last_currency a,
(
    select  a2.*, a1.value_curs, a1.date_change
    from    cash_CURS_STD a1, cash_currency_std a2
    where   a1.is_deleted = 0 and
            a1.id_std_curr = a2.id_std_curr and
            a1.date_change = (
                select  max(z1.DATE_CHANGE)
                from    cash_CURS_STD z1
                where   a1.ID_STD_CURR = z1.ID_STD_CURR and
                        z1.is_deleted = 0
            )
) c
where   b.id_currency = a.id_currency(+) and
        b.id_standart_curs = c.id_std_curr(+)
</Text>
			</View>
			<View schema="MILLENNIUM" name="F_D_V_CASH_SELECT_LAST_CURR" type="VIEW">
				<Text xmlns="">select  id_currency, date_change, curs
from    cash_curr_value a
where   date_change = (
    select  max(f.date_change) last_date
    from    cash_curr_value f
    where   f.id_currency = a.id_currency
    )
</Text>
			</View>
